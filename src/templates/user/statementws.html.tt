<div id="statement-ws-container">
    <div class="page-title" id="statement-title">
        <h1>[% l('Statement') %]</h1>
        <br>
        <p class="notice-msg center-text invisible" id="error-msg"></p>
    </div>
    <div class="gr-row gr-padding-10 invisible">
        <div class="gr-6 gr-12-m">
            <div class="gr-row padding-left">
                <div class="label_form">
                    <label>[% l("Start from:") %]</label>
                </div>
                <input type="text" maxlength="10" id="jump-to" size="10" readonly="readonly"/>
            </div>
        </div>
        <div class="gr-6 gr-12-m align-end">
            <div id="download_csv" class="invisible">[% l('[_1] rows displayed:', '<span id="rows_count"></span>') %] <a href="javascript:;">[% l('Download CSV') %]</a></div>
        </div>
    </div>
</div>
