<div class="container container">
  <div class="static_full">
    <h1>
      [% l("Partners") %]
    </h1>
    <p>
      [% l("[_1] provides business partnership services via the following programs.", website_name) %]
    </p>
  </div>
  <div class="gr-row gr-clear">
    <div class="gr-4 gr-12-m">
      <div class="gr-6 gr-padding-10">
        <a class="pjaxload" href="[% request.url_for('/affiliate/signup') %]">
          <img class="responsive charting-page-icon" id="charting_icon" src="[% request.url_for('images/pages/partners/affiliate-program.svg') %]"/>
        </a>
      </div>
      <div class="gr-12">
        <h4>
          <a class="pjaxload" href="[% request.url_for('/affiliate/signup') %]">
            [% l("Affiliate Program") %]
          </a>
        </h4>
        <p>
          [% l("Earn commissions by introducing new customers to [_1].", website_name) %]
        </p>
      </div>
    </div>
    <div class="gr-4 gr-12-m">
      <div class="gr-6 gr-padding-10">
        <a class="pjaxload" href="[% request.url_for('/payment-agent') %]">
          <img class="responsive charting-page-icon" id="charting_icon" src="[% request.url_for('images/pages/partners/payment-agents.svg') %]"/>
        </a>
      </div>
      <div class="gr-12">
        <h4>
          <a class="pjaxload" href="[% request.url_for('/payment-agent') %]">
            [% l("Payment Agent") %]
          </a>
        </h4>
        <p>
          [% l("Apply to become a Payment Agent") %]
        </p>
      </div>
    </div>
    <div class="gr-4 gr-12-m">
      <div class="gr-6 gr-padding-10">
        <a href="https://developers.binary.com" target="_blank">
          <img class="responsive charting-page-icon" id="charting_icon" src="[% request.url_for('images/pages/partners/api.svg') %]"/>
        </a>
      </div>
      <div class="gr-12">
        <h4>
          <a href="https://developers.binary.com" target="_blank">
            [% l("API") %]
          </a>
        </h4>
        <p>
          [% l("Develop your own trading application with our APIs.") %]
        </p>
      </div>
    </div>
  </div>
  <div class="gr-row gr-clear">
    <div class="gr-4 gr-12-m">
      <div class="gr-6 gr-padding-10">
        <a class="pjaxload" href="[% request.url_for('/open-source-projects') %]">
          <img class="responsive charting-page-icon" id="charting_icon" src="[% request.url_for('images/pages/partners/open-source-venture.svg') %]"/>
        </a>
      </div>
      <div class="gr-12">
        <h4>
          <a class="pjaxload" href="[% request.url_for('/open-source-projects') %]">
            [% l("Open Source") %]
          </a>
        </h4>
        <p>
          [% l("Contribute to [_1]\'s open-source projects.", website_name) %]
        </p>
      </div>
    </div>
    <div class="gr-4 gr-12-m">
      <div class="gr-6 gr-padding-10">
        <a href="http://blog.binary.com/en/contributors/" target="_blank">
          <img class="responsive charting-page-icon" id="charting_icon" src="[% request.url_for('images/pages/partners/contributors.svg') %]" target="_blank"/>
        </a>
      </div>
      <div class="gr-12">
        <h4>
          <a href="https://blog.binary.com/en/contributors/" target="_blank">
            [% l("Contributors") %]
          </a>
        </h4>
        <p>
          [% l("Apply to become a contributor") %]
        </p>
      </div>
    </div>
    <div class="gr-4 gr-12-m">
      <div class="gr-6 gr-padding-10">
        <a class="pjaxload" href="[% request.url_for('/user-testing') %]">
          <img class="responsive charting-page-icon" id="charting_icon" src="[% request.url_for('images/pages/partners/tester.svg') %]"/>
        </a>
      </div>
      <div class="gr-12">
        <h4>
          <a class="pjaxload" href="[% request.url_for('/user-testing') %]">
            [% l("User Testing") %]
          </a>
        </h4>
        <p>
          [% l("Earn money for any verifiable errors that you find.") %]
        </p>
      </div>
    </div>
  </div>
</div>
